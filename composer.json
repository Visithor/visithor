{
    "name": "visithor/visithor",
    "description": "Visithor for PHP",
    "keywords": ["visithor", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/yaml": "~2.1",
        "symfony/console": "~2.1",
        "symfony/stopwatch": "~2.1",

        "guzzlehttp/guzzle": "~5.2"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.4.2",
        "mmoreram/php-formatter": "1.0.4",
        "phpunit/phpunit": "4.5.0"
    },
    "bin": [
        "bin/visithor"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Visithor\\": "src/Visithor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Visithor\\Tests\\": "tests/Visithor"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
